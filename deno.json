{
  "nodeModulesDir": "auto",
  "imports": {
    "lodash": "https://cdn.skypack.dev/lodash?dts",
    "path": "https://deno.land/std@0.138.0/path/mod.ts",
    "routine": "https://deno.land/x/routine/mod.ts",
    "@std/assert": "jsr:@std/assert@1"
  },
  "tasks": {
    "run": "deno run --allow-read --allow-write --allow-scripts=npm:numjs --allow-ffi",
    "run-watch": "deno run --watch --allow-read --allow-write --allow-scripts=npm:numjs --allow-ffi",
    "test": "deno test --watch --allow-import --allow-read --allow-scripts=npm:numjs --allow-ffi"
  }
}
